[32m+[m[32m    @Override[m
[32m+[m[32m    public void onResume() {[m
[32m+[m	[32m// TODO Auto-generated method stub[m
[32m+[m	[32msuper.onResume();[m
[32m+[m	[32mSet_Referash_Data();[m
[32m+[m
[32m+[m[32m    }[m
