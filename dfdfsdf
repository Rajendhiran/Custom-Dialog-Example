[32m+[m[32m    public void Call_My_Blog(View v) {[m
[32m+[m	[32mIntent intent = new Intent(Main_Screen.this, My_Blog.class);[m
[32m+[m	[32mstartActivity(intent);[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void Set_Referash_Data() {[m
[32m+[m	[32mcontact_data.clear();[m
[32m+[m	[32mdb = new DatabaseHandler(this);[m
[32m+[m	[32mArrayList<Contact> contact_array_from_db = db.Get_Contacts();[m
[32m+[m
[32m+[m	[32mfor (int i = 0; i < contact_array_from_db.size(); i++) {[m
[32m+[m
[32m+[m	[32m    int tidno = contact_array_from_db.get(i).getID();[m
[32m+[m	[32m    String name = contact_array_from_db.get(i).getName();[m
[32m+[m	[32m    String mobile = contact_array_from_db.get(i).getPhoneNumber();[m
[32m+[m	[32m    String email = contact_array_from_db.get(i).getEmail();[m
[32m+[m	[32m    Contact cnt = new Contact();[m
[32m+[m	[32m    cnt.setID(tidno);[m
[32m+[m	[32m    cnt.setName(name);[m
[32m+[m	[32m    cnt.setEmail(email);[m
[32m+[m	[32m    cnt.setPhoneNumber(mobile);[m
[32m+[m
[32m+[m	[32m    contact_data.add(cnt);[m
[32m+[m	[32m}[m
[32m+[m	[32mdb.close();[m
[32m+[m	[32mcAdapter = new Contact_Adapter(Main_Screen.this, R.layout.listview_row,[m
[32m+[m		[32mcontact_data);[m
[32m+[m	[32mContact_listview.setAdapter(cAdapter);[m
[32m+[m	[32mcAdapter.notifyDataSetChanged();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void Show_Toast(String msg) {[m
[32m+[m	[32mToast.makeText(getApplicationContext(), msg, Toast.LENGTH_LONG).show();[m
[32m+[m[32m    }[m
