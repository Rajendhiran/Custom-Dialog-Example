[32m+[m[32m    public void Is_Valid_Sign_Number_Validation(int MinLen, int MaxLen,[m
[32m+[m	[32m    EditText edt) throws NumberFormatException {[m
[32m+[m	[32mif (edt.getText().toString().length() <= 0) {[m
[32m+[m	[32m    edt.setError("Number Only");[m
[32m+[m	[32m    valid_mob_number = null;[m
[32m+[m	[32m} else if (edt.getText().toString().length() < MinLen) {[m
[32m+[m	[32m    edt.setError("Minimum length " + MinLen);[m
[32m+[m	[32m    valid_mob_number = null;[m
[32m+[m
[32m+[m	[32m} else if (edt.getText().toString().length() > MaxLen) {[m
[32m+[m	[32m    edt.setError("Maximum length " + MaxLen);[m
[32m+[m	[32m    valid_mob_number = null;[m
[32m+[m
[32m+[m	[32m} else {[m
[32m+[m	[32m    valid_mob_number = edt.getText().toString();[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m    } // END OF Edittext validation[m
[32m+[m
[32m+[m[32m    public void Is_Valid_Email(EditText edt) {[m
[32m+[m	[32mif (edt.getText().toString() == null) {[m
[32m+[m	[32m    edt.setError("Invalid Email Address");[m
[32m+[m	[32m    valid_email = null;[m
[32m+[m	[32m} else if (isEmailValid(edt.getText().toString()) == false) {[m
[32m+[m	[32m    edt.setError("Invalid Email Address");[m
[32m+[m	[32m    valid_email = null;[m
[32m+[m	[32m} else {[m
[32m+[m	[32m    valid_email = edt.getText().toString();[m
[32m+[m	[32m}[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    boolean isEmailValid(CharSequence email) {[m
[32m+[m	[32mreturn android.util.Patterns.EMAIL_ADDRESS.matcher(email).matches();[m
[32m+[m[32m    } // end of email matcher[m
[32m+[m
[32m+[m[32m    public void Is_Valid_Person_Name(EditText edt) throws NumberFormatException {[m
[32m+[m	[32mif (edt.getText().toString().length() <= 0) {[m
[32m+[m	[32m    edt.setError("Accept Alphabets Only.");[m
[32m+[m	[32m    valid_name = null;[m
[32m+[m	[32m} else if (!edt.getText().toString().matches("[a-zA-Z ]+")) {[m
[32m+[m	[32m    edt.setError("Accept Alphabets Only.");[m
[32m+[m	[32m    valid_name = null;[m
[32m+[m	[32m} else {[m
[32m+[m	[32m    valid_name = edt.getText().toString();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void Show_Toast(String msg) {[m
[32m+[m	[32mToast.makeText(getApplicationContext(), msg, Toast.LENGTH_LONG).show();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void Reset_Text() {[m
