[32m+[m[32m    public class Contact_Adapter extends ArrayAdapter<Contact> {[m
[32m+[m	[32mActivity activity;[m
[32m+[m	[32mint layoutResourceId;[m
[32m+[m	[32mContact user;[m
[32m+[m	[32mArrayList<Contact> data = new ArrayList<Contact>();[m
[32m+[m
[32m+[m	[32mpublic Contact_Adapter(Activity act, int layoutResourceId,[m
[32m+[m		[32mArrayList<Contact> data) {[m
[32m+[m	[32m    super(act, layoutResourceId, data);[m
[32m+[m	[32m    this.layoutResourceId = layoutResourceId;[m
[32m+[m	[32m    this.activity = act;[m
[32m+[m	[32m    this.data = data;[m
[32m+[m	[32m    notifyDataSetChanged();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@Override[m
[32m+[m	[32mpublic View getView(int position, View convertView, ViewGroup parent) {[m
[32m+[m	[32m    View row = convertView;[m
[32m+[m	[32m    UserHolder holder = null;[m
[32m+[m
[32m+[m	[32m    if (row == null) {[m
[32m+[m		[32mLayoutInflater inflater = LayoutInflater.from(activity);[m
[32m+[m
[32m+[m		[32mrow = inflater.inflate(layoutResourceId, parent, false);[m
[32m+[m		[32mholder = new UserHolder();[m
[32m+[m		[32mholder.name = (TextView) row.findViewById(R.id.user_name_txt);[m
[32m+[m		[32mholder.email = (TextView) row.findViewById(R.id.user_email_txt);[m
[32m+[m		[32mholder.number = (TextView) row.findViewById(R.id.user_mob_txt);[m
[32m+[m		[32mholder.edit = (Button) row.findViewById(R.id.btn_update);[m
[32m+[m		[32mholder.delete = (Button) row.findViewById(R.id.btn_delete);[m
[32m+[m		[32mrow.setTag(holder);[m
[32m+[m	[32m    } else {[m
[32m+[m		[32mholder = (UserHolder) row.getTag();[m
[32m+[m	[32m    }[m
[32m+[m	[32m    user = data.get(position);[m
[32m+[m	[32m    holder.edit.setTag(user.getID());[m
[32m+[m	[32m    holder.delete.setTag(user.getID());[m
[32m+[m	[32m    holder.name.setText(user.getName());[m
[32m+[m	[32m    holder.email.setText(user.getEmail());[m
[32m+[m	[32m    holder.number.setText(user.getPhoneNumber());[m
[32m+[m
[32m+[m	[32m    holder.edit.setOnClickListener(new OnClickListener() {[m
[32m+[m
[32m+[m		[32m@Override[m
[32m+[m		[32mpublic void onClick(View v) {[m
[32m+[m		[32m    // TODO Auto-generated method stub[m
[32m+[m		[32m    Log.i("Edit Button Clicked", "**********");[m
[32m+[m
[32m+[m		[32m    Intent update_user = new Intent(activity,[m
[32m+[m			[32m    Add_Update_User.class);[m
[32m+[m		[32m    update_user.putExtra("called", "update");[m
[32m+[m		[32m    update_user.putExtra("USER_ID", v.getTag().toString());[m
[32m+[m		[32m    activity.startActivity(update_user);[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m	[32m    });[m
[32m+[m	[32m    holder.delete.setOnClickListener(new OnClickListener() {[m
[32m+[m
[32m+[m		[32m@Override[m
[32m+[m		[32mpublic void onClick(final View v) {[m
[32m+[m		[32m    // TODO Auto-generated method stub[m
[32m+[m
[32m+[m		[32m    // show a message while loader is loading[m
[32m+[m
[32m+[m		[32m    AlertDialog.Builder adb = new AlertDialog.Builder(activity);[m
[32m+[m		[32m    adb.setTitle("Delete?");[m
[32m+[m		[32m    adb.setMessage("Are you sure you want to delete ");[m
[32m+[m		[32m    final int user_id = Integer.parseInt(v.getTag().toString());[m
[32m+[m		[32m    adb.setNegativeButton("Cancel", null);[m
[32m+[m		[32m    adb.setPositiveButton("Ok",[m
[32m+[m			[32m    new AlertDialog.OnClickListener() {[m
[32m+[m				[32m@Override[m
[32m+[m				[32mpublic void onClick(DialogInterface dialog,[m
[32m+[m					[32mint which) {[m
[32m+[m				[32m    // MyDataObject.remove(positionToRemove);[m
[32m+[m				[32m    DatabaseHandler dBHandler = new DatabaseHandler([m
[32m+[m					[32m    activity.getApplicationContext());[m
[32m+[m				[32m    dBHandler.Delete_Contact(user_id);[m
[32m+[m				[32m    Main_Screen.this.onResume();[m
[32m+[m
[32m+[m				[32m}[m
[32m+[m			[32m    });[m
[32m+[m		[32m    adb.show();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m    });[m
[32m+[m	[32m    return row;[m
[32m+[m
[32m+[m	[32m}[m
