[32m+[m[32mpackage chintan.khetiya.sqlite.cursor;[m
[32m+[m
[32m+[m[32mimport java.util.ArrayList;[m
[32m+[m
[32m+[m[32mimport android.content.ContentValues;[m
[32m+[m[32mimport android.content.Context;[m
[32m+[m[32mimport android.database.Cursor;[m
[32m+[m[32mimport android.database.sqlite.SQLiteDatabase;[m
[32m+[m[32mimport android.database.sqlite.SQLiteOpenHelper;[m
[32m+[m[32mimport android.util.Log;[m
[32m+[m
[32m+[m[32mpublic class DatabaseHandler extends SQLiteOpenHelper {[m
[32m+[m
[32m+[m[32m    // All Static variables[m
[32m+[m[32m    // Database Version[m
[32m+[m[32m    private static final int DATABASE_VERSION = 1;[m
[32m+[m
[32m+[m[32m    // Database Name[m
[32m+[m[32m    private static final String DATABASE_NAME = "contactsManager";[m
[32m+[m
[32m+[m[32m    // Contacts table name[m
[32m+[m[32m    private static final String TABLE_CONTACTS = "contacts";[m
[32m+[m
[32m+[m[32m    // Contacts Table Columns names[m
[32m+[m[32m    private static final String KEY_ID = "id";[m
[32m+[m[32m    private static final String KEY_NAME = "name";[m
[32m+[m[32m    private static final String KEY_PH_NO = "phone_number";[m
[32m+[m[32m    private static final String KEY_EMAIL = "email";[m
[32m+[m[32m    private final ArrayList<Contact> contact_list = new ArrayList<Contact>();[m
[32m+[m
[32m+[m[32m    public DatabaseHandler(Context context) {[m
[32m+[m	[32msuper(context, DATABASE_NAME, null, DATABASE_VERSION);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Creating Tables[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onCreate(SQLiteDatabase db) {[m
[32m+[m	[32mString CREATE_CONTACTS_TABLE = "CREATE TABLE " + TABLE_CONTACTS + "("[m
[32m+[m		[32m+ KEY_ID + " INTEGER PRIMARY KEY," + KEY_NAME + " TEXT,"[m
[32m+[m		[32m+ KEY_PH_NO + " TEXT," + KEY_EMAIL + " TEXT" + ")";[m
[32m+[m	[32mdb.execSQL(CREATE_CONTACTS_TABLE);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Upgrading database[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onUpgrade(SQLiteDatabase db, int oldVersion, int newVersion) {[m
[32m+[m	[32m// Drop older table if existed[m
[32m+[m	[32mdb.execSQL("DROP TABLE IF EXISTS " + TABLE_CONTACTS);[m
[32m+[m
[32m+[m	[32m// Create tables again[m
[32m+[m	[32monCreate(db);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * All CRUD(Create, Read, Update, Delete) Operations[m
[32m+[m[32m     */[m
[32m+[m
[32m+[m[32m    // Adding new contact[m
[32m+[m[32m    public void Add_Contact(Contact contact) {[m
[32m+[m	[32mSQLiteDatabase db = this.getWritableDatabase();[m
[32m+[m	[32mContentValues values = new ContentValues();[m
[32m+[m	[32mvalues.put(KEY_NAME, contact.getName()); // Contact Name[m
[32m+[m	[32mvalues.put(KEY_PH_NO, contact.getPhoneNumber()); // Contact Phone[m
[32m+[m	[32mvalues.put(KEY_EMAIL, contact.getEmail()); // Contact Email[m
[32m+[m	[32m// Inserting Row[m
[32m+[m	[32mdb.insert(TABLE_CONTACTS, null, values);[m
[32m+[m	[32mdb.close(); // Closing database connection[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Getting single contact[m
[32m+[m[32m    Contact Get_Contact(int id) {[m
[32m+[m	[32mSQLiteDatabase db = this.getReadableDatabase();[m
[32m+[m
[32m+[m	[32mCursor cursor = db.query(TABLE_CONTACTS, new String[] { KEY_ID,[m
[32m+[m		[32mKEY_NAME, KEY_PH_NO, KEY_EMAIL }, KEY_ID + "=?",[m
[32m+[m		[32mnew String[] { String.valueOf(id) }, null, null, null, null);[m
[32m+[m	[32mif (cursor != null)[m
[32m+[m	[32m    cursor.moveToFirst();[m
[32m+[m
[32m+[m	[32mContact contact = new Contact(Integer.parseInt(cursor.getString(0)),[m
[32m+[m		[32mcursor.getString(1), cursor.getString(2), cursor.getString(3));[m
[32m+[m	[32m// return contact[m
[32m+[m	[32mcursor.close();[m
[32m+[m	[32mdb.close();[m
[32m+[m
[32m+[m	[32mreturn contact;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Getting All Contacts[m
[32m+[m[32m    public ArrayList<Contact> Get_Contacts() {[m
[32m+[m	[32mtry {[m
[32m+[m	[32m    contact_list.clear();[m
[32m+[m
[32m+[m	[32m    // Select All Query[m
[32m+[m	[32m    String selectQuery = "SELECT  * FROM " + TABLE_CONTACTS;[m
[32m+[m
[32m+[m	[32m    SQLiteDatabase db = this.getWritableDatabase();[m
[32m+[m	[32m    Cursor cursor = db.rawQuery(selectQuery, null);[m
[32m+[m
[32m+[m	[32m    // looping through all rows and adding to list[m
[32m+[m	[32m    if (cursor.moveToFirst()) {[m
[32m+[m		[32mdo {[m
[32m+[m		[32m    Contact contact = new Contact();[m
[32m+[m		[32m    contact.setID(Integer.parseInt(cursor.getString(0)));[m
[32m+[m		[32m    contact.setName(cursor.getString(1));[m
[32m+[m		[32m    contact.setPhoneNumber(cursor.getString(2));[m
[32m+[m		[32m    contact.setEmail(cursor.getString(3));[m
[32m+[m		[32m    // Adding contact to list[m
[32m+[m		[32m    contact_list.add(contact);[m
[32m+[m		[32m} while (cursor.moveToNext());[m
[32m+[m	[32m    }[m
[32m+[m
[32m+[m	[32m    // return contact list[m
[32m+[m	[32m    cursor.close();[m
[32m+[m	[32m    db.close();[m
[32m+[m	[32m    return contact_list;[m
[32m+[m	[32m} catch (Exception e) {[m
[32m+[m	[32m    // TODO: handle exception[m
[32m+[m	[32m    Log.e("all_contact", "" + e);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn contact_list;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Updating single contact[m
[32m+[m[32m    public int Update_Contact(Contact contact) {[m
[32m+[m	[32mSQLiteDatabase db = this.getWritableDatabase();[m
[32m+[m
[32m+[m	[32mContentValues values = new ContentValues();[m
[32m+[m	[32mvalues.put(KEY_NAME, contact.getName());[m
[32m+[m	[32mvalues.put(KEY_PH_NO, contact.getPhoneNumber());[m
[32m+[m	[32mvalues.put(KEY_EMAIL, contact.getEmail());[m
[32m+[m
[32m+[m	[32m// updating row[m
[32m+[m	[32mreturn db.update(TABLE_CONTACTS, values, KEY_ID + " = ?",[m
[32m+[m		[32mnew String[] { String.valueOf(contact.getID()) });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Deleting single contact[m
[32m+[m[32m    public void Delete_Contact(int id) {[m
[32m+[m	[32mSQLiteDatabase db = this.getWritableDatabase();[m
[32m+[m	[32mdb.delete(TABLE_CONTACTS, KEY_ID + " = ?",[m
[32m+[m		[32mnew String[] { String.valueOf(id) });[m
[32m+[m	[32mdb.close();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Getting contacts Count[m
[32m+[m[32m    public int Get_Total_Contacts() {[m
[32m+[m	[32mString countQuery = "SELECT  * FROM " + TABLE_CONTACTS;[m
[32m+[m	[32mSQLiteDatabase db = this.getReadableDatabase();[m
[32m+[m	[32mCursor cursor = db.rawQuery(countQuery, null);[m
[32m+[m	[32mcursor.close();[m
[32m+[m
[32m+[m	[32m// return count[m
[32m+[m	[32mreturn cursor.getCount();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m}[m
